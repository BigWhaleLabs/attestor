{
  "name": "zk-proof-generator",
  "version": "1.0.0",
  "description": "ZK proof generator",
  "main": "dist/server.js",
  "repository": "https://github.com/BigWhaleLabs/zk-proof-generator",
  "license": "MIT",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "distribute-server": "node dist/server.js",
    "distribute-checker": "node dist/checker.js",
    "distribute": "yarn build && concurrently 'yarn distribute-server' 'yarn distribute-checker'",
    "develop": "concurrently 'yarn develop-server' 'yarn develop-checker'",
    "develop-server": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
    "develop-checker": "tsc-watch --skipLibCheck --onSuccess 'node dist/checker.js'",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "@koa/cors": "^3.3.0",
    "@noble/secp256k1": "^1.5.5",
    "@zk-kit/incremental-merkle-tree": "^0.4.3",
    "amala": "^8.0.2",
    "blake-hash": "^2.0.0",
    "blake2b": "^2.1.4",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "ethers": "^5.6.5",
    "ffjavascript": "^0.2.52",
    "keccak256": "^1.0.6",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "mongoose": "^6.3.3",
    "snarkjs": "^0.4.16",
    "web3-utils": "^1.7.3"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^9.8.1",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.3.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@3.1.0"
}
